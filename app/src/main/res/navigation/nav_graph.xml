<?xml version="1.0" encoding="utf-8"?>

<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">


    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.desafioevaluado.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_resultFragment"
            app:destination="@id/resultFragment" />
    </fragment>

    <fragment
        android:id="@+id/resultFragment"
        android:name="com.example.desafioevaluado.ResultFragment"
        android:label="fragment_result"
        tools:layout="@layout/fragment_result" />
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.desafioevaluado.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
</navigation>